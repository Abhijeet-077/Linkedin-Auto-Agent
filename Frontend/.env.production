# Production Environment Configuration for GitHub Pages
# This file is used for production builds

# Use mock API for GitHub Pages deployment (no backend required)
VITE_USE_MOCK_API=true

# Backend API URL - Update this with your actual backend URL if available
VITE_API_BASE_URL=https://your-backend-api.vercel.app

# Application metadata
VITE_APP_TITLE=LinkedIn Forge AutoAgent
VITE_APP_DESCRIPTION=AI-powered LinkedIn content generation and management platform

# GitHub Pages specific configuration
VITE_GITHUB_PAGES=true

# Disable debug logging in production
VITE_DEBUG=false

# Production optimizations
VITE_NODE_ENV=production
